# Clusterwide, changing namespace is allowed
---
kind: SealedSecret
apiVersion: bitnami.com/v1alpha1
metadata:
  name: {{ tls_certificates.wildcard_stamx_nl.name }}
  namespace: {{ tls_wildcard_stamx_nl_namespace }}
  annotations:
    sealedsecrets.bitnami.com/cluster-wide: "true"
spec:
  encryptedData:
    tls.crt: 
    tls.key: 
  template:
    type: kubernetes.io/tls
    metadata:
      name: {{ tls_certificates.wildcard_stamx_nl.name }}
      namespace: {{ tls_wildcard_stamx_nl_namespace }}
      annotations:
        sealedsecrets.bitnami.com/cluster-wide: "true"
